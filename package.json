{
  "name": "rn-library",
  "version": "0.0.0",
  "description": "Hello Expo!",
  "author": null,
  "main": "main.js",
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js",
    "flow": "flow; test $? -eq 1 -o $? -eq 2",
    "flow:precommit": "flow; test $? -eq 1 -o $? -eq 2 && exit 1 || exit 0"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/ex-navigation": "~2.10.0",
    "@expo/samples": "~1.0.3",
    "expo": "16.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-16.0.0.tar.gz"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.44.2",
    "jest-expo": "0.4.0",
    "pre-commit": "^1.2.2"
  },
  "precommit.silent": true,
  "pre-commit": [
    "flow:precommit"
  ]
}
