{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "standard"
    ],
    "env": {
      "browser": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
        "react-native/no-unused-styles": 2,
        "react/prop-types": 0,
        "comma-dangle": 0,
        "no-var": 2,
        "no-undef": 2
    },
    "globals": {
      "fetch": true
    }
}
